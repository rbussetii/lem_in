#include "lem_in.h"

t_lem	*ft_create_lem(char *infos)
{
	t_lem	*elem;

	elem->next = NULL;
	elem->prev = NULL;
	elem->lem_nbr = 0;
	elem->start = 0;
	elem->end = 0;
	elem->weight = 0;
	elem->name = NULL;
	return (elem);
}
